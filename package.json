{
    "name": "financiera",
    "version": "1.0",
    "description": "Sistema de Gestion Financiera",
    "main": "index.js",
    "scripts": {
        "client-install": "cd front/ && npm install",
        "server-install": "cd back/ && npm install",       
        "financiera-install": "npm install",       
                
        "server": " cd back/ && npm run dev",
        "client": " cd front/ && npm run dev ",
        
        "serverB": " cd back/ && npm run start",
        "clientB": " cd front/ && npm run build",

        "serverStart": " cd back/ && npm run start -p 5000",
        "clientStart": " cd front/ && npm run start -p 3000",

        "dev": "concurrently \"npm run server\" \"npm run client\"",        
        "build": "concurrently \"npm run clientB\"",
        "start": "concurrently \"npm run serverStart\" \"npm run clientStart\""

    },
    "author": "Ing. Longo Victor Manuel <victor.m.longo@gmail.com>",
    "license": "MIT",
    "browserslist": [
        ">0.2%",
        "not dead",
        "not op_mini all"
    ],
    "dependencies": {
        "concurrently": "^7.3.0",
        "react-icons": "^4.4.0"
    }
}
